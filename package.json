{
  "name": "aziz-bhai-invoice-admin",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "test": "cross-env NODE_ENV=test next dev",
    "build:prod": "next build",
    "start:prod": "next start",
    "build:test": "cross-env NODE_ENV=test next build",
    "start:test": "cross-env NODE_ENV=test next start",
    "lint": "next lint",
    "prepare": "cd .. && husky AfterMarket-Frontend/.install",
    "check-types": "tsc --pretty --noEmit",
    "format": "prettier --write .",
    "build-all": "yarn check-format && yarn prisma:generate && yarn build",
    "tsc": "tsc -p ./tsconfig.json",
    "prisma:generate:dev": "npx dotenv -e .env.development -- npx prisma generate dev",
    "prisma:migrate:dev": "npx dotenv -e .env.development -- npx prisma migrate dev --preview-feature",
    "prisma:pull:dev": "npx dotenv -e .env.development -- npx prisma db pull && npx dotenv -e .env.development -- npx prisma generate",
    "prisma:generate:test": "npx dotenv -e .env.test -- npx prisma generate test",
    "prisma:migrate:test": "npx dotenv -e .env.test -- npx prisma migrate dev --preview-feature",
    "prisma:pull:test": "npx dotenv -e .env.test -- npx prisma db pull && npx dotenv -e .env.test -- npx prisma generate",
    "prisma:generate:prod": "dotenv -e .env.production -- npx prisma generate prod",
    "prisma:migrate:prod": "npx dotenv -e .env.production -- npx prisma migrate dev --preview-feature",
    "prisma:pull:prod": "npx dotenv -e .env.production -- npx prisma db pull && npx dotenv -e .env.production -- npx prisma generate"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.338.0",
    "@aws-sdk/s3-request-presigner": "^3.335.0",
    "@emotion/cache": "^11.9.3",
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "@mui/icons-material": "^5.8.4",
    "@mui/material": "^5.9.1",
    "@mui/x-date-pickers": "^5.0.1",
    "@prisma/client": "^5.18.0",
    "@reduxjs/toolkit": "^1.8.3",
    "@tanstack/react-query": "^5.51.1",
    "axios": "^1.3.3",
    "bcrypt": "^5.1.0",
    "currency-list": "^1.0.8",
    "date-fns": "^2.29.1",
    "dayjs": "^1.11.6",
    "devextreme": "^22.1.3",
    "devextreme-react": "^22.1.3",
    "exceljs": "^4.3.0",
    "formik": "^2.2.9",
    "jsonwebtoken": "^9.0.0",
    "next": "13",
    "nextjs-progressbar": "^0.0.16",
    "passport": "^0.6.0",
    "passport-local": "^1.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-dropzone": "^14.2.2",
    "react-medium-image-zoom": "^5.1.2",
    "react-phone-number-input": "^3.3.9",
    "react-redux": "^8.0.2",
    "react-select-country-list": "^2.2.3",
    "react-toastify": "^9.1.1",
    "redux-persist": "^6.0.0",
    "sharp": "^0.33.5",
    "use-debounce": "^10.0.2",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@types/bcrypt": "^5.0.2",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^18.14.0",
    "@types/passport": "^1.0.16",
    "@types/passport-local": "^1.0.38",
    "@types/react": "^18.3.3",
    "@types/react-dom": "18.0.6",
    "@types/react-select-country-list": "^2.2.3",
    "@types/yup": "^0.32.0",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^7.4.2",
    "eslint": ">=5.16.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-jsx-a11y": "^6.10.0",
    "eslint-plugin-react": "^7.30.1",
    "husky": "^8.0.3",
    "prettier": "^2.7.1",
    "prisma": "^5.18.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4"
  }
}
